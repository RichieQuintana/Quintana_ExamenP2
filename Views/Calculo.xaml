<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Quintana_AppApuntes.Views.Calculo"
             Title="Calculo">
    <StackLayout>
        <Entry x:Name="entrada" Placeholder="Ingrese una cadena" />
        <Button Text="Calcular" Clicked="Calcular_Clicked" />
        <Label x:Name="resultado" />
        <Button Text="Limpiar" Clicked="Limpiar_Clicked" />
    </StackLayout>
</ContentPage>